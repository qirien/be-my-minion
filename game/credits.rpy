label credits:
    # TODO: Updated License.txt with whatever license we decide to use, and if additional resources are added.
    scene black with fade

    show text "{color=fae5cc}Written by Bridget Loyola and Andrea Landaker{/font}"
    $ renpy.pause(2.0)
    
    show text "{color=fae5cc}Sprites, CGs, and GUI by Anne{/font}"
    $ renpy.pause(2.0)
    
    show text "{color=fae5cc}Music\n\nLa valse du oni from Démo musique de film by BLS\nMephisto Polka by Franz Liszt, performed by Sofja Gülbadamova\nTema de los Malos by clbustos\nEvil Princess Theme & Awkward Mage Theme by Wes Landaker\nSetting The Sails by Marc Teichert\nCello by Bo Moonlight\nPompous by Celestial Aeon Project{/font}"
    $ renpy.pause(4.0)
    
    show text "{color=fae5cc}Backgrounds\n\nBy Limp Nyanko\naquamary\nDougitDesign\nSylwia77\n\nRain Texture by Neonescence{/font}"
    $ renpy.pause(3.0)
    
    show text "{color=fae5cc}Sounds\n\nBy Freesound.org users wjl, Kodack, ihitokage, 211redman112, Mixedupmoviestuff, D W, kbnevel, DJ Chronos, homejrande{/font}"
    $ renpy.pause(2.0)

    show text "{color=fae5cc}Testing\n\nWes Landaker\n{/font}" #TODO: add testers here
    $ renpy.pause(2.0)

    show expression "epilogue" + " zoomin" at top with fade
    $ renpy.pause(1.6)
    
    scene black with fade
    return
    
