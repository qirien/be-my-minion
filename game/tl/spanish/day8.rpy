translate spanish niir8_d78fe0e5:

    # p angry "Do you trust yourself so little? Fortunately, I trust you more."
    p angry "¿Tan poco confía en usted? Por suerte, yo confío más en usted."

# game/day8.rpy:305
translate spanish niir8_855a0093:

