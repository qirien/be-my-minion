    # p angry "Where is it?!"
    p angry "¿Dónde está?"

# game/day7.rpy:186
translate spanish cyril7_642e0aa6:

    # c "Oh! On your feet, your Highness!"
    c "¡Vaya! ¡En sus pies, Su Alteza!"

# game/day7.rpy:187
